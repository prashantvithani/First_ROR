<%= link_to("<< Back to section list", {:action => 'section_list', :id => @page.id}, :class => 'back-link')%>

<div class = "section show">
	<h2>section Details</h2>
	<table summary = "section detail view"> 
		<tr>
			<th>Name</th>
			<td><%= @section.name %></td>
		</tr>
		<tr>
			<th>Position</th>
			<td><%= @section.position%></td>
		</tr>
		<tr>
			<th>Visiblity</th>
			<td><%= @section.visible ? 'true' : 'false'%></td>
		</tr>
		<tr>
			<th>Content_type</th>
			<td><%= @section.content_type%></td>
		</tr>
		<tr>
			<th>Created</th>
			<td><%= @section.created_at%></td>
		</tr>
		<tr>
			<th>Updated</th>
			<td><%= @section.updated_at%></td>
		</tr>

	</table>
	<%= params.inspect%>
</div>